<script>
  $(document).ready(function(){
    var $carousel = $('.carousel-container');

    // Initialize the carousel
    $carousel.slick({
      autoplay: true, 
      autoplaySpeed: 4000, 
      dots: true,
    });

    
    var debouncedAdvanceCarousel = _.debounce(function() {
      $carousel.slick('slickNext'); // Advance to the next slide
    }, 1000); // Adjust the delay as needed (in milliseconds)

    // Listen for user activity events and trigger carousel advancement
    $(document).on('mousemove keydown touchstart', function() {
      debouncedAdvanceCarousel();
    });
  });
</script>
